<routes>
	<vTypeDistribution id="CarDist">
		<vType id="CarA" accel="2.6" decel="4.9" carFollowModel="Wiedemann" security="1.0" length="5" minGap="5" maxSpeed="17.0" probability="0.05"/>
		<vType id="CarB" accel="2.6" decel="5.3" carFollowModel="Wiedemann" security="1.0" length="5" minGap="5" maxSpeed="18.5" probability="0.10"/>
		<vType id="CarC" accel="2.6" decel="5.1" carFollowModel="Wiedemann" security="1.0" length="5" minGap="5" maxSpeed="20.0" probability="0.25"/>
		<vType id="CarD" accel="2.6" decel="5.4" carFollowModel="Wiedemann" security="1.0" length="5" minGap="5" maxSpeed="21.0" probability="0.25"/>
		<vType id="CarE" accel="2.6" decel="5.2" carFollowModel="Wiedemann" security="1.0" length="5" minGap="5" maxSpeed="22.0" probability="0.20"/>
		<vType id="CarF" accel="2.6" decel="5.6" carFollowModel="Wiedemann" security="1.0" length="5" minGap="5" maxSpeed="23.5" probability="0.10"/>
		<vType id="CarG" accel="2.6" decel="5.8" carFollowModel="Wiedemann" security="1.0" length="5" minGap="5" maxSpeed="25.0" probability="0.05"/>
	</vTypeDistribution>
	<route id="route0" edges="E1 E2"></route>
	<route id="route1" edges="-E2 -E1"></route>
	<!-- CarA: "0,0,1"=blue; CarB: "0,1,0"=green; CarC: "0,1,1"=cyan; CarD: "1,0,0"=red; CarE: "1,0,1"=magenta; CarF: "1,1,0"=yellow; CarG: "1,1,1"=white -->
	<flow id="flow0" type="CarDist" route="route0" begin="0" end="1000" probability="0.1" departSpeed="12" color="1,1,0"></flow>
	<flow id="flow1" type="CarDist" route="route1" begin="0" end="1000" probability="0.1" departSpeed="12" color="1,1,0"></flow>
</routes>
