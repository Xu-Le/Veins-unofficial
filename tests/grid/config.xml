<?xml version="1.0" encoding="UTF-8"?>
<root>
	<AnalogueModels>
		<AnalogueModel type="SimplePathlossModel">
			<parameter name="alpha" type="double" value="2.0"/>
			<parameter name="carrierFrequency" type="double" value="5.890e+9"/>
		</AnalogueModel>
	</AnalogueModels>
	<Decider type="Decider80211p">
		<!-- The center frequency on which the phy listens-->
		<parameter name="centerFrequency" type="double" value="5.890e9"/>
	</Decider>
	<RoutingPlans>
		<!-- Ensure simtime is written increasely, or simulation will crash, however the order of RoutingPlan doesn't matter -->
		<RoutingPlan type="4">
			<parameter name="simtime" type="double" value="90.0"/>
			<parameter name="receiver" type="long" value="16"/>
		</RoutingPlan>
		<RoutingPlan type="14">
			<parameter name="simtime" type="double" value="145.0"/>
			<parameter name="receiver" type="long" value="11"/>
		</RoutingPlan>
		<RoutingPlan type="18">
			<parameter name="simtime" type="double" value="185.0"/>
			<parameter name="receiver" type="long" value="19"/>
		</RoutingPlan>
	</RoutingPlans>
	<WarningPlans>
		<!-- Notes: Ensure simtime is written increasely, or simulation will crash, however the order of WarningPlan doesn't matter, and the value of simtime should not be the integer since mobility updating through TraCI is executed after sending warning message if they are set at the same simtime, the warning message's distance of vehicles can be both positive and negative, the latter indicates the warning message aims to vehicles which are driving opposite since vehicle possibly turn around, whereas the warning message's distance of RSUs can only be positive -->
		<WarningPlan type="3">
			<!--
			<parameter name="simtime" type="double" value="100.1"/>
			<parameter name="distance" type="double" value="600"/>
			<parameter name="simtime" type="double" value="190.1"/>
			<parameter name="distance" type="double" value="600"/>
			-->
		</WarningPlan>
		<!-- RSUs' type start from 10000, make sure keep consistent with macro RSU_ADDRESS_OFFSET in BaseRSU.h -->
		<!--
		<WarningPlan type="10000">
			<parameter name="simtime" type="double" value="120.1"/>
			<parameter name="distance" type="double" value="600"/>
		</WarningPlan>
		<WarningPlan type="10001">
			<parameter name="simtime" type="double" value="150.1"/>
			<parameter name="distance" type="double" value="1000"/>
			<parameter name="simtime" type="double" value="180.1"/>
			<parameter name="distance" type="double" value="1000"/>
		</WarningPlan>
		<WarningPlan type="10002">
			<parameter name="simtime" type="double" value="105.1"/>
			<parameter name="distance" type="double" value="750"/>
		</WarningPlan>
		-->
	</WarningPlans>
</root>
