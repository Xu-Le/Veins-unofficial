<routes>
	<vTypeDistribution id="CarDist">
		<vType id="CarA" accel="2.6" decel="4.5" carFollowModel="Krauss" sigma="1.0" length="5" minGap="5" maxSpeed="19.5" probability="0.10"/>
		<vType id="CarB" accel="2.6" decel="4.5" carFollowModel="Krauss" sigma="1.0" length="5" minGap="5" maxSpeed="20.8" probability="0.25"/>
		<vType id="CarC" accel="2.6" decel="4.5" carFollowModel="Krauss" sigma="1.0" length="5" minGap="5" maxSpeed="22.2" probability="0.30"/>
		<vType id="CarD" accel="2.6" decel="4.5" carFollowModel="Krauss" sigma="1.0" length="5" minGap="5" maxSpeed="23.6" probability="0.25"/>
		<vType id="CarE" accel="2.6" decel="4.5" carFollowModel="Krauss" sigma="1.0" length="5" minGap="5" maxSpeed="25.0" probability="0.10"/>
	</vTypeDistribution>
	<route id="route0" edges="E1 E2"></route>
	<route id="route1" edges="-E2 -E1"></route>
	<!-- CarA: "0,0,1"=blue; CarB: "0,1,0"=green; CarC: "0,1,1"=cyan; CarD: "1,0,0"=red; CarE: "1,0,1"=magenta -->
	<flow id="flow0" type="CarDist" route="route0" begin="0" end="1800" probability="0.025" departSpeed="16.7" color="1,1,0"></flow>
	<flow id="flow1" type="CarDist" route="route1" begin="0" end="1800" probability="0.025" departSpeed="16.7" color="1,1,0"></flow>
</routes>
